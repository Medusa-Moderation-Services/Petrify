{
  "name": "Petrify",
  "tree": {
    "$className": "DataModel",
    
    "ServerStorage": {
      "PetrifyLibrary": {
        "$path": "src/PetrifyLibrary"
      }
    },

    "ServerScriptService": {
      "UPLOAD-TO-PUBLIC-PACKAGE": {
        "$path": "src/Petrify-Data"
      }
    },
    "ReplicatedStorage": {
      "PetrifyLibrary": {
        "$path": "src/PetrifyLibrary"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
{
  "name": "Petrify",
  "tree": {
    "$className": "DataModel",
    
    "ServerStorage": {
      "PetrifyLibrary": {
        "$path": "src/PetrifyLibrary"
      }
    },

    "ServerScriptService": {
      "Petrify": {
        "$path": "src/Petrify",
        "PetrifyLibrary": {
          "$path": "src/PetrifyLibrary"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}